  },
  "dependencies": {
    "@ant-design/icons": "^5.5.0",
    "antd": "^5.19.1",
    "axios": "^1.12.2",
    "better-sqlite3": "^8.7.0",
    "exceljs": "^4.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
